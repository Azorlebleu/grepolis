{
   "name": "integration-tests",
   "version": "1.0.2",
   "description": "Tests",
   "scripts": {
      "delete:reports": "IF exist cypress\\reports ( echo Folder exists : cleaning it && rd /Q /S cypress\\reports && mkdir cypress\\reports ) ELSE ( mkdir cypress\\reports && echo Created a new folder )",
      "prereport": "npm run delete:reports",
      "make_merged_report": "npx mochawesome-merge --reportDir cypress/reports/ > cypress/mochareports/report.json",
      "tests_cypress": "cypress run run --spec 'cypress/integration/*.js' --reporter cypress-multi-reporters --reporter-options configFile=reporter-config.json",
      "report": "npm run tests_cypress",
      "api-test-validation-entreprise": "newman run tests/CI_Entreprise.postman_collection.json -e env/validation_main.postman_environment.json --reporter-junit-export newman.xml --reporters junit,cli",
